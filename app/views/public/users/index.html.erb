<h1>users</h1>
<% @users.each do |user| %>
  <%= link_to user_path(user.id) do %>
    <!--退会しているユーザーは表示させない↓-->
    <% if user.is_deleted == false %>
      <%= image_tag user.get_profile_image(100,100), class: "rounded-circle" %>
      <%= user.name %>

      <!--フォローボタン-->
      <div id="follow-<%= user.id %>">
        <%= render 'public/relationships/follow', user: user %>
      </div>

    <% end %>
  <% end %>
<% end %>
